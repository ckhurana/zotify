<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <ListPreference
        android:defaultValue="@string/pref_course_btech_cs_value"
        android:entries="@array/pref_course_options"
        android:entryValues="@array/pref_course_values"
        android:key="@string/pref_course_key"
        android:title="@string/pref_course_title">

    </ListPreference>

    <ListPreference
        android:defaultValue="@string/pref_notif_3"
        android:entries="@array/pref_notif_options"
        android:entryValues="@array/pref_notif_values"
        android:key="@string/pref_sync_freq_key"
        android:title="@string/pref_sync_freq_title">

    </ListPreference>

    <ListPreference
        android:defaultValue="@string/pref_tab_gen_v"
        android:entries="@array/pref_tab_options"
        android:entryValues="@array/pref_tab_values"
        android:key="@string/pref_tab_key"
        android:title="@string/pref_tab_title">

    </ListPreference>

    <CheckBoxPreference
        android:defaultValue="@string/pref_enable_notifications_default"
        android:key="@string/pref_enable_notifications_key"
        android:summaryOff="@string/pref_enable_notifications_false"
        android:summaryOn="@string/pref_enable_notifications_true"
        android:title="@string/pref_enable_notifications_title">

    </CheckBoxPreference>


</PreferenceScreen>